{
    "name": "ijodkor/laravel-api-response",
    "description": "Response util for Laravel",
    "type": "metapackage",
    "keywords": [
        "php",
        "laravel",
        "api",
        "response"
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Ijodkor\\LaravelApiResponse\\": "src/"
        }
    },
